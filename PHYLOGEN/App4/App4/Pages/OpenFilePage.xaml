<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App4.Pages.OpenFilePage">
     <Label Text="Open a File: " VerticalOptions="Center" HorizontalOptions="Center" />
     <Entry x:Name="FileEntryBox" Text="Browse..." Completed="OnFileSelected"></Entry>
     <Button x:Name="FileOpenBtn" Text="OPEN" BorderColor="Gray"></Button>
</ContentPage>